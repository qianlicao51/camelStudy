<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
	http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd 
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd 
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<routeContext id="sysHttp" xmlns="http://camel.apache.org/schema/spring">
	<route>
		<from uri="jetty:http://0.0.0.0/myapp/1.jpg?sessionSupport=true" />
		<!-- <removeHeaders pattern="CamelHttp*" /> -->
		<to uri="bean:httpHandle" />
	</route>

	<route>
		<from uri="jetty:http://0.0.0.0:8080/app/1.jpg" />
		<removeHeaders pattern="CamelHttp*" />
		<to uri="bean:httpHandle2" />
	</route>
</routeContext>
</beans>
